/**
     * @description method to setup test data
     * @param  null
     * @return null
     */	 
    
    @TestSetup
    public static void setupData(){
        AITM_Pricing_Basis__c pb = new AITM_Pricing_Basis__c(
                                Name ='None - Air BP not Represented',
                                AITM_Description__c = 'test',
                                AITM_Currency__c = 'USD',
                                AITM_Unit_Of_Measure__c = 'USG'
                                );
      upsert pb;
    
        Set<String> countryId = new Set<String>();
        countryId.add('10490');
        AITM_MetaDataUtility mu = new AITM_MetaDataUtility();
        mu.createMetadata(countryId);
               
       Account acc = new Account(Name = 'NA - This is a Location Contact', RecordTypeId = Schema.SObjectType.Account.getRecordTypeInfosByDeveloperName().get('AirBP_Customer').getRecordTypeId());
       upsert acc;
               
    }
    /**
     * @description test method for batch
     * @param  null
     * @return null
     */	 
    @isTest
    public static void testAITM_AirBPCountryAPIBatch(){
    setupData();
        String countryId = '10490';
        Test.setMock(HttpCalloutMock.class, new AITM_AirBPCountryAPIBatchSuccessResponseMock());
        AITM_AirBPCountryAPIBatch bp = new AITM_AirBPCountryAPIBatch();        
        Test.startTest();
            Database.executeBatch(bp);
        Test.stopTest();
        System.assertEquals('10490', countryId);
        
    }
    
